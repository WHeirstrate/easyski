    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "knex": "^0.95.15",
    "nodemon": "^2.0.15",
    "objection": "^3.0.1",
    "pg": "^8.7.1"
  }
}
